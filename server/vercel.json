{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [{ "src": "/api/(.*)", "dest": "/index.js" }],
  "env": {
    "NODE_ENV": "production"
  }
}
